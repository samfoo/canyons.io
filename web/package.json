{
  "name": "canyons-web",
  "version": "0.1.0",
  "private": true,
  "description": "A website for canyoneers to record their adventures.",
  "main": "start.js",
  "author": "Sam Gibson",
  "license": "GPL-3.0",
  "scripts": {
    "client": "./node_modules/.bin/webpack",
    "styles": "./node_modules/.bin/lessc --verbose --no-js --strict-imports src/components/application.less ./public/site.css",
    "watch-client": "./node_modules/.bin/webpack --watch",
    "watch-styles": "fswatch -i '\\.less' -o src | xargs -n1 -I{} npm run-script styles",
    "serve": "DEBUG=canyons nodemon --use_strict server.js",
    "lint": "./node_modules/.bin/eslint src/**/*.js"
  },
  "dependencies": {
    "axios": "^0.7.0",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "cookie-parser": "^1.4.0",
    "express": "^4.13.3",
    "history": "^1.12.5",
    "immutable": "^3.7.5",
    "morgan": "^1.6.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^3.1.0",
    "react-router": "1.0.0-rc3",
    "redux": "^3.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "chai": "^3.3.0",
    "debug": "^2.2.0",
    "eslint": "^1.6.0",
    "less": "^2.5.3",
    "mocha": "^2.3.3",
    "nodemon": "^1.7.1",
    "normalize.css": "^3.0.3",
    "webpack": "^1.12.2"
  }
}
