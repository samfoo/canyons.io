{
  "name": "canyons-web",
  "version": "0.1.0",
  "private": true,
  "description": "A website for canyoners to record their adventures.",
  "main": "server.js",
  "author": "Sam Gibson",
  "license": "GPL-3.0",
  "scripts": {
    "serve": "DEBUG=canyons node --use_strict server.js",
    "compile": "./node_modules/babel-cli/bin/babel.js -d lib/ src/"
  },
  "dependencies": {
    "axios": "^0.7.0",
    "babel": "^6.3.13",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.21",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "cookie-parser": "^1.4.0",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "font-awesome": "^4.4.0",
    "google-map-react": "^0.9.3",
    "history": "^1.12.5",
    "immutable": "^3.7.5",
    "less": "^2.5.3",
    "models": "file:../models",
    "morgan": "^1.6.1",
    "normalize.css": "^3.0.3",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-redux": "^3.1.0",
    "react-router": "1.0.0-rc3",
    "redux": "^3.0.2",
    "simplify-js": "^1.2.1",
    "togeojson": "^0.13.0",
    "webpack": "^1.12.2"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "jest-cli": "^0.8.0",
    "mocha": "^2.3.4",
    "proxyquire": "^1.7.3",
    "react-addons-test-utils": "^0.14.2",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "babel": {
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ],
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "jest": {
    "scriptPreprocessor": "./test-preprocessor.js",
    "unmockedModulePathPatterns": [
      "./node_modules/immutable",
      "./node_modules/react",
      "./node_modules/react-addons-test-utils",
      "./node_modules/react-dom",
      "./node_modules/react-redux"
    ]
  }
}
